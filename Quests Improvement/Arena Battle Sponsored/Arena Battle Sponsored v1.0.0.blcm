							<hotfix name="Reward2X" level="None">
								<code profiles="default">set GD_Z1_BanditSlaughter.M_BanditSlaughter1 Reward.RewardItems (WeaponBalanceDefinition'GD_Weap_Pistol.A_Weapons.Pistol_Bandit_3_Rare',WeaponBalanceDefinition'GD_Weap_Pistol.A_Weapons.Pistol_Bandit_3_Rare')</code>
							</hotfix>
