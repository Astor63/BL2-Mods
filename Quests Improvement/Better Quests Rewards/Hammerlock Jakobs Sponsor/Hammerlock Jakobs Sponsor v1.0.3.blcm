						<hotfix name="Reward2X" level="None">
							<code profiles="default">set GD_Sage_SM_NowYouSeeIt.M_Sage_NowYouSeeIt Reward.RewardItems (WeaponBalanceDefinition'GD_Sage_Weapons.Shotgun.SG_Jakobs_3_Hydra',WeaponBalanceDefinition'GD_Sage_Weapons.Shotgun.SG_Jakobs_3_Hydra',)</code>
						</hotfix>
					</category>
